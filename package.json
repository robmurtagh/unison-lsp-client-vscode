{
  "name": "unison",
  "displayName": "Unison",
  "description": "Unison Language support for Visual Studio Code",
  "author": "Robert Murtagh",
  "preview": true,
  "license": "MIT",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/robmurtagh/unison-lsp-client-vscode"
  },
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#FFFFFF",
    "theme": "light"
  },
  "publisher": "unisonweb",
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "unison",
    "unisonweb"
  ],
  "activationEvents": [
    "onLanguage:unison"
  ],
  "engines": {
    "vscode": "^1.43.0"
  },
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "unison",
        "extensions": [
          ".u"
        ]
      }
    ]
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -watch"
  },
  "dependencies": {
    "vscode-languageclient": "^6.1.3"
  },
  "devDependencies": {
    "@types/vscode": "1.43.0",
    "typescript": "^3.9.5",
    "vsce": "^1.77.0",
    "vscode-test": "^1.3.0"
  }
}
