{
  "name": "unison",
  "displayName": "Unison",
  "description": "Unison Language support for Visual Studio Code",
  "author": "Robert Murtagh",
  "preview": true,
  "license": "MIT",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/robmurtagh/unison-lsp-client-vscode"
  },
  "icon": "images/icon.png",
  "galleryBanner": {
    "color": "#FFFFFF",
    "theme": "light"
  },
  "publisher": "unisonweb",
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "unison",
    "unisonweb"
  ],
  "activationEvents": [
    "onLanguage:unison"
  ],
  "engines": {
    "vscode": "^1.43.0"
  },
  "main": "./out/extension.js",
  "contributes": {
    "languages": [
      {
        "id": "unison",
        "extensions": [
          ".u"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "unison",
        "scopeName": "source.u",
        "path": "./syntaxes/unison.tmLanguage.json"
      }
    ]
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -watch",
    "package": "echo 'npm run package is a temporary command!' && vsce package -o ~/Desktop/",
    "launch": "echo 'npm run launch is a temporary command!' && code --install-extension ~/Desktop/unison-0.0.1.vsix"
  },
  "dependencies": {
    "vscode-languageclient": "^6.1.3"
  },
  "devDependencies": {
    "@types/vscode": "1.43.0",
    "typescript": "^3.9.5",
    "vsce": "^1.77.0",
    "vscode-test": "^1.3.0"
  }
}
